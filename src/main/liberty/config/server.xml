<server description="dada Liberty server">
    <!-- tag::featureManager[] -->
    <featureManager>
        <feature>servlet-4.0</feature>
        <feature>cdi-2.0</feature>
    </featureManager>
    <!-- end::featureManager[] -->

    <!-- tag::httpEndpoint[] -->
    <httpEndpoint httpPort="${default.http.port}" httpsPort="${default.https.port}"
                  id="defaultHttpEndpoint" host="*"/>
    <!-- end::httpEndpoint[] -->

    <!-- tag::webApplication[] -->
    <webApplication location="dada-web.war" contextRoot="${app.context.root}"/>-->
    <!-- end::webApplication[] -->
</server>